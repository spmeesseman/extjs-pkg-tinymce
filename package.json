{
	"name": "extjs-pkg-tinymce",
	"displayName": "ExtJs Server TinyMCE Package",
	"description": "ExtJS 6 TinyMCE Package",
	"publisher": "spmeesseman",
	"version": "1.0.5",
	"icon": "images/subversion.png",
	"homepage": "https://github.com/spmeesseman/extjs-pkg-tinymce/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/spmeesseman/extjs-pkg-tinymce.git"
	},
	"bugs": {
		"url": "https://github.com/spmeesseman/extjs-pkg-tinymce/issues"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^3.0.2",
		"@semantic-release/git": "^7.0.8",
		"@semantic-release/exec": "^3.3.0",
		"commitizen": "^3.0.7",
		"cz-conventional-changelog": "^2.1.0",
		"semantic-release": "^15.13.3"
	},
	"scripts": {
		"install2": "npm run install",
		"build": "echo success",
		"ant-build": "ant -logger org.apache.tools.ant.listener.AnsiColorLogger build-tinymce",
		"repo-release": "npx sencha package add build\\tinymce\\tinymce.pkg",
		"semantic-release": "semantic-release --no-ci",
		"semantic-release-dry": "semantic-release --dry-run"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"release": {
		"preset": "angular",
		"plugins": [
			[
				"@semantic-release/commit-analyzer",
				{
					"releaseRules": [
						{
							"type": "docs",
							"scope": "readme",
							"release": "patch"
						},
						{
							"type": "refactor",
							"release": "patch"
						},
						{
							"type": "style",
							"release": "patch"
						}
					],
					"parserOpts": {
						"noteKeywords": [
							"BREAKING CHANGE",
							"BREAKING CHANGES"
						]
					}
				}
			],
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/changelog",
				{
					"changelogTitle": "#Change Log"
				}
			],
			"@semantic-release/npm",
			"@semantic-release/github",
			"@semantic-release/git"
		]
	},
	"sencha": {
		"name": "tinymce",
		"namespace": "Ext",
		"framework": "ext",
		"type": "code",
		"toolkit": "classic",
		"theme": "theme-neptune",
		"creator": "smeesseman",
		"summary": "Package wrapper for TinyMCE",
		"detailedDescription": "Package wrapper for TinyMCE Content Editor",
		"version": "4.8.2.2",
		"compatVersion": "4.2.0",
		"format": "1",
		"slicer": null,
		"output": "${framework.dir}/build/packages/${package.name}",
		"local": true,
		"resource": {
			"paths": ""
		},
		"resources": [
			{
				"path": "${package.dir}/resources",
				"output": "base"
			}
		],
		"sass": {
			"namespace": "Ext",
			"etc": [
				"${package.dir}/sass/var"
			],
			"var": [
				"${package.dir}/sass/var"
			],
			"src": [
				"${package.dir}/sass/src",
				"${package.dir}/sass/src/main.scss"
			]
		},
		"classpath": [
			"${package.dir}/src"
		],
		"overrides": [
			"${package.dir}/overrides"
		],
		"requires": [
			"core"
		],
		"ignore": [
			"(^|/)CVS(/?$|/.*?$)"
		]
	}
}
