{
    "name": "@perryjohnson/extjs-pkg-tinymce",
    "version": "1.0.12",
    "displayName": "ExtJS Package Wrapper for TinyMCE Editor",
    "description": "ExtJS Package Wrapper for TinyMCE Wysiwyg Editor",
    "license": "GPL-3.0",
    "author": {
        "name": "Scott Meesseman"
    },
    "bugs": {
        "url": "https://app1.development.pjats.com/projects/set_project.php?project=extjs-pkg-tinymce&make_default=no&ref=bug_report_page.php"
    },
    "homepage": "https://app1.development.pjats.com/svn/web/listing.php?repname=pja&path=%2Fextjs-pkg-tinymce%2Ftrunk%2F",
    "repository": {
        "type": "svn",
        "url": "https://svn.development.pjats.com/pja/extjs-pkg-tinymce/trunk"
    },
    "devDependencies": {},
    "keywords": [
        "sencha extjs",
        "tinymce",
        "wysiwyg",
        "html editor"
    ],
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "src",
        "resources"
    ],
    "sencha": {
        "name": "tinymce",
        "namespace": "Ext",
        "framework": "ext",
        "type": "code",
        "toolkit": "classic",
        "theme": "theme-neptune",
        "creator": "smeesseman",
        "summary": "Package wrapper for TinyMCE",
        "detailedDescription": "Package wrapper for TinyMCE Content Editor",
        "version": "4.8.2.2",
        "compatVersion": "4.2.0",
        "format": "1",
        "slicer": null,
        "output": "${framework.dir}/build/packages/${package.name}",
        "local": true,
        "resource": {
            "paths": ""
        },
        "resources": [
            {
                "path": "${package.dir}/resources",
                "output": "base"
            }
        ],
        "sass": {
            "namespace": "Ext",
            "etc": [
                "${package.dir}/sass/var"
            ],
            "var": [
                "${package.dir}/sass/var"
            ],
            "src": [
                "${package.dir}/sass/src",
                "${package.dir}/sass/src/main.scss"
            ]
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "requires": [
            "core"
        ],
        "ignore": [
            "(^|/)CVS(/?$|/.*?$)"
        ]
    },
    "dependencies": {}
}
